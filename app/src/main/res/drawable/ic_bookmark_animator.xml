<?xml version="1.0" encoding="utf-8"?>
<vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">

    <path
        android:name="empty"
        android:pathData="@string/bookmark_empty"
        android:fillColor="?attr/lightTextColor" />

    <clip-path
        android:name="clip"
        android:pathData="@string/bookmark_clip_hidden" />

    <path
        android:name="full"
        android:pathData="@string/bookmark_filled"
        android:fillColor="?attr/fabColor" />

</vector>